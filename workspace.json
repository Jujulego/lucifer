{
  "version": 1,
  "projects": {
    "lucifer-api": {
      "root": "apps/lucifer-api",
      "sourceRoot": "apps/lucifer-api/src",
      "projectType": "application",
      "prefix": "lucifer-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/lucifer-api",
            "main": "apps/lucifer-api/src/main.ts",
            "tsConfig": "apps/lucifer-api/tsconfig.app.json",
            "assets": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "lucifer-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/lucifer-api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/lucifer-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "lucifer-front": {
      "root": "apps/lucifer-front",
      "sourceRoot": "apps/lucifer-front/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/lucifer-front",
            "index": "apps/lucifer-front/src/index.html",
            "main": "apps/lucifer-front/src/main.tsx",
            "polyfills": "apps/lucifer-front/src/polyfills.ts",
            "tsConfig": "apps/lucifer-front/tsconfig.app.json",
            "assets": [
              "apps/lucifer-front/src/favicon.ico",
              "apps/lucifer-front/src/assets"
            ],
            "styles": ["apps/lucifer-front/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/lucifer-front/src/environments/environment.ts",
                  "with": "apps/lucifer-front/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "lucifer-front:build",
            "proxyConfig": "apps/lucifer-front/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "lucifer-front:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/lucifer-front/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/lucifer-front/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "lucifer-front-e2e": {
      "root": "apps/lucifer-front-e2e",
      "sourceRoot": "apps/lucifer-front-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/lucifer-front-e2e/cypress.json",
            "tsConfig": "apps/lucifer-front-e2e/tsconfig.e2e.json",
            "devServerTarget": "lucifer-front:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "lucifer-front:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/lucifer-front-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "lucifer-api",
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  }
}
