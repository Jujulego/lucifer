{
  "version": 1,
  "projects": {
    "auth0": {
      "root": "apps/auth0",
      "sourceRoot": "apps/auth0/src",
      "projectType": "application",
      "prefix": "auth0-rules",
      "architect": {
        "build": {
          "builder": ".:tsc-build",
          "outputs": [
            "res/rules"
          ],
          "options": {
            "tsConfig": "apps/auth0/tsconfig.app.json"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/auth0/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/auth0"
          ],
          "options": {
            "jestConfig": "apps/auth0/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "lucifer-api": {
      "root": "apps/lucifer-api",
      "sourceRoot": "apps/lucifer-api/src",
      "projectType": "application",
      "prefix": "lucifer-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/lucifer-api",
            "main": "apps/lucifer-api/src/main.ts",
            "tsConfig": "apps/lucifer-api/tsconfig.app.json",
            "assets": [
              "apps/lucifer-api/src/Procfile"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "lucifer-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/lucifer-api/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/lucifer-api/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/lucifer-api"
          ]
        },
        "deploy": {
          "builder": ".:deploy",
          "options": {
            "buildPath": "dist/apps/lucifer-api",
            "repository": "https://github.com/Jujulego/lucifer.git"
          }
        },
        "db-migrate": {
          "builder": ".:db-migrate"
        },
        "gen-migration": {
          "builder": ".:gen-migration"
        }
      }
    },
    "lucifer-front": {
      "root": "apps/lucifer-front",
      "sourceRoot": "apps/lucifer-front/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/lucifer-front",
            "index": "apps/lucifer-front/src/index.html",
            "main": "apps/lucifer-front/src/main.tsx",
            "polyfills": "apps/lucifer-front/src/polyfills.ts",
            "tsConfig": "apps/lucifer-front/tsconfig.app.json",
            "assets": [
              "apps/lucifer-front/src/favicon.ico",
              "apps/lucifer-front/src/assets",
              "apps/lucifer-front/src/Procfile"
            ],
            "styles": [
              "apps/lucifer-front/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/lucifer-front/src/environments/environment.ts",
                  "with": "apps/lucifer-front/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "e2e": {
              "fileReplacements": [
                {
                  "replace": "apps/lucifer-front/src/environments/environment.ts",
                  "with": "apps/lucifer-front/src/environments/environment.e2e.ts"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "lucifer-front:build",
            "proxyConfig": "apps/lucifer-front/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "lucifer-front:build:production"
            },
            "e2e": {
              "buildTarget": "lucifer-front:build:e2e"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/lucifer-front/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/lucifer-front/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/lucifer-front"
          ]
        },
        "deploy": {
          "builder": ".:deploy",
          "options": {
            "buildPath": "dist/apps/lucifer-front",
            "repository": "https://github.com/Jujulego/lucifer.git"
          }
        }
      }
    },
    "lucifer-front-e2e": {
      "root": "apps/lucifer-front-e2e",
      "sourceRoot": "apps/lucifer-front-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/lucifer-front-e2e/cypress.json",
            "tsConfig": "apps/lucifer-front-e2e/tsconfig.e2e.json",
            "devServerTarget": "lucifer-front:serve:e2e"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/lucifer-front-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "react-api": {
      "root": "libs/react/api",
      "sourceRoot": "libs/react/api/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/react/api/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/react/api/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/react/api"
          ]
        }
      }
    },
    "react-basics": {
      "root": "libs/react/basics",
      "sourceRoot": "libs/react/basics/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/react/basics/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/react/basics/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/react/basics"
          ]
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/react/basics/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/react-basics",
            "config": {
              "configFolder": "libs/react/basics/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        }
      }
    },
    "react-fields": {
      "root": "libs/react/fields",
      "sourceRoot": "libs/react/fields/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/react/fields/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/react/fields/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/react/fields"
          ]
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/react/fields/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/react-fields",
            "config": {
              "configFolder": "libs/react/fields/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        }
      }
    },
    "react-table": {
      "root": "libs/react/table",
      "sourceRoot": "libs/react/table/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
