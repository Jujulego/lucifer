    },
    "react-utils": {
      "root": "libs/react/utils",
      "sourceRoot": "libs/react/utils/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/react/utils/**/*.{ts,tsx,js,jsx}"
            ]
          }
