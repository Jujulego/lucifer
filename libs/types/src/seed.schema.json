{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://lucifer.herokuapp.com/seed.schema.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "users": {
      "type": "array",
      "description": "Users to create (must already exists in auth0)",
      "items": {
        "$ref": "user/schema.json"
      }
    },
    "projects": {
      "type": "array",
      "description": "Projects to create",
      "items": {
        "$ref": "project/schema.json"
      }
    },
    "variables": {
      "type": "array",
      "description": "Variables to create",
      "items": {
        "$ref": "variable/schema.json"
      }
    }
  }
}
