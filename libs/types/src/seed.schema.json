{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://lucifer.herokuapp.com/seed.schema.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "users": {
      "type": "array",
      "description": "Users to create (must already exists in auth0)",
      "items": {
        "$ref": "lib/user.schema.json"
      }
    },
    "projects": {
      "type": "array",
      "description": "Projects to create",
      "items": {
        "$ref": "lib/project.schema.json"
      }
    },
    "machines": {
      "type": "array",
      "description": "Machines to create",
      "items": {
        "$ref": "lib/machine.schema.json"
      }
    }
  }
}
