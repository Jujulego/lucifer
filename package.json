{
  "name": "lucifer",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "builders": "./tools/executors/executors.json",
  "engines": {
    "node": "14.x"
  },
  "workspaces": [
    "apps/*",
    "libs/react/*",
    "libs/*"
  ],
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "build:tools": "tsc -p tools/tsconfig.tools.json",
    "test": "nx run-many --all --target=test",
    "lint": "nx workspace-lint && nx run-many --all --target=lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "workspace-generator": "nx workspace-generator",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn run typeorm migration:generate",
    "storybook": "nx run storybook:storybook",
    "postinstall": "is-ci || husky install",
    "heroku-postbuild": "bash ./scripts/heroku-postbuild.sh"
  },
  "dependencies": {
    "class-validator": "^0.13.1",
    "document-register-element": "^1.14.10",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1102.4",
    "@nrwl/cli": "11.5.1",
    "@nrwl/cypress": "11.5.1",
    "@nrwl/devkit": "11.5.1",
    "@nrwl/eslint-plugin-nx": "11.5.1",
    "@nrwl/jest": "11.5.1",
    "@nrwl/nest": "11.5.1",
    "@nrwl/node": "11.5.1",
    "@nrwl/nx-cloud": "11.2.0",
    "@nrwl/react": "11.5.1",
    "@nrwl/storybook": "11.5.1",
    "@nrwl/web": "11.5.1",
    "@nrwl/workspace": "11.5.1",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addons": "^6.1.17",
    "@storybook/react": "^6.1.21",
    "@storybook/theming": "^6.1.21",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "@types/ci-info": "^3.1.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.33",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "ci-info": "^3.1.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^5.1.3",
    "is-ci": "^3.0.0",
    "jest": "^26.6.3",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.2.3",
    "wait-on": "^5.2.1"
  }
}
